<%= form_with(model: student, data: { controller: 'validation' }) do |form| %>
  <div class="form-group">
      <%= form.label :classroom_id, class: 'form-control-label' %>
        <%= form.collection_select(:classroom_id, Classroom.all, :id, :to_s, {}, { class: 'form-control', data: { controller: 'select' } }) %>
    </div>
<div class="card-footer pl-0">
  <%= form.submit class: 'btn btn-success' %>
</div>
<% end %>
